{
	"description": "Testing documentation, and demo framework for tw5-relink",
	"plugins": [
		"flibbles/relink",
		"flibbles/relink-fieldnames",
		"tiddlywiki/jasmine",
		"tiddlywiki/railroad"
	],
	"themes": [
		"tiddlywiki/vanilla",
		"tiddlywiki/snowwhite"
	],
	"build": {
		"test": [
			"--output", "./docs/",
			"--rendertiddler",
				"$:/core/save/all",
				"test.html",
				"text/plain",
				"",
				"publishFilter",
				"-$:/DefaultTiddlers -[prefix[$:/state]] -[!is[system]]",
			"--test"
		],
		"index": [
			"--output", "./docs/",
			"--rendertiddler",
				"$:/core/save/all",
				"index.html",
				"text/plain",
				"",
				"publishFilter",
				"-[prefix[$:/test/]] -$:/plugins/tiddlywiki/jasmine -[prefix[$:/plugins/flibbles/relink]] -[prefix[$:/UpgradeLibrary]]"
		]
	}
}
