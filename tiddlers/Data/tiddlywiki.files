{
	"directories": [
		{
			"path": ".",
			"filesRegExp": "^.*\\.tid$",
			"isTiddlerFile": true,
			"searchSubdirectories": true,
			"fields": {
				"title": {
					"source": "basename"
				}
			}
		},
		{
			"path": ".",
			"filesRegExp": "^.*\\.svg$",
			"isTiddlerFile": false,
			"searchSubdirectories": true,
			"fields": {
				"title": {
					"source": "filename"
				},
				"type": "image/svg+xml"

			}
		}
	]
}
