\whitespace trim

<$graph>
 <$graph.popup>
  <$properties $filter="[tag[Norse]]" color="#8b0000" >
  <$properties $filter="[tag[Ruler]]" shape=star >
   <$list filter="[tag[Deity]!is[system]!is[draft]]">
    <$node shape={{!!shape}} label={{!!title}} doubleclick="<$action-navigate/>"/>
    <$edges.typed />
   </$list>
