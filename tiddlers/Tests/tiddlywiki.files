{
	"directories": [
		{
			"path": ".",
			"filesRegExp": "^.*\\.js$",
			"isTiddlerFile": true,
			"fields": {
				"tags": "$:/tags/test-spec",
				"title": {
					"source": "filename",
					"prefix": "$:/test/"
				},
				"type": "application/javascript"
			}
		},
		{
			"path": "./relink",
			"filesRegExp": "^.*\\.js$",
			"isTiddlerFile": true,
			"fields": {
				"tags": "$:/tags/relink-test-spec",
				"title": {
					"source": "filename",
					"prefix": "$:/test/relink/"
				},
				"type": "application/javascript"
			}
		},
		{
			"path": "./modules",
			"filesRegExp": "^.*\\.js$",
			"isTiddlerFile": true,
			"fields": {
				"title": {
					"source": "filename",
					"prefix": "$:/test/modules/"
				},
				"type": "application/javascript"
			}
		}
	]
}
