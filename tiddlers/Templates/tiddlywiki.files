{
	"directories": [
		{
			"path": ".",
			"filesRegExp": "^.*\\.tid$",
			"isTiddlerFile": true,
			"fields": {
				"tags": "Templates",
				"name": {
					"source": "basename"
				},
				"caption": {
					"prefix": "{{$:/plugins/flibbles/graph/templates/",
					"suffix": "!!caption}}",
					"source": "basename"
				}
			}
		}
	]
}
